{
  "contracts": ["contracts/**"],
  "buildDir": "dist/",
  "config": {
    "contracts": "embarkConfig/contracts.js",
    "blockchain": "embarkConfig/blockchain.js",
    "storage": "embarkConfig/storage.js",
    "namesystem": false,
    "communication": false,
    "webserver": false
  },
  "versions": {
    "web3": "1.0.0-beta",
    "solc": "0.4.18",
    "ipfs-api": "17.2.4"
  },
  "plugins": {
    "embarkjs-connector-web3": {},
    "@trailofbits/embark-contract-info": {
      "flags": ""
    }
  },
  "options": {
    "solc": {
      "optimize": true,
      "optimize-runs": 200
    }
  },
  "generationDir": "src/embarkArtifacts"
}
